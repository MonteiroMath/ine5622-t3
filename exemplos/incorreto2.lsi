def exemploCorreto(int a, int b){
  
  int c, d, e, f;

  c := a * b;
  d := b / a;
  e := c + d;
  f := b - a;

  if (e >= c){
    if (f <= d)
      return c;
    else {
      return a;
    }
  } else {
    return f;
  } 
  return a;
  
}

def main(){
  int a, b;
  a := 3;
  b := 15;

  c := exemploCorreto(a, b);

  return c;
}