def ifSemBloco(int a, int b ){

  int c, d;
  
  if(a == 1){
    c := a + b;
  }

  if (a == 2){
    c := a-b;
  }

  if (a == 3)
    c := a*b;

  if (a == 4){
    c := a/b;
  }

  if (a == 5){
    c := a == b;
  }

  if (a == 6){
    c := a <> b;
  }

  return c;
}

def main(){

  int a, b, c;

  a := 3;
  b := 5;
  c := ifSemBloco(a,b);
  print(c);

}